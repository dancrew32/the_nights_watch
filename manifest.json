{
	"name": "The Night's $watch",
	"version": "0.0.1",
	"description": "A $watchers analyzer for AngularJS + jQuery",
	"homepage_url": "https://github.com/dancrew32/the_nights_watch",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"icons": {"128": "icon.png"},
	"permissions": ["<all_urls>"],
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
